<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/eventListener.xsd">
	<import>
		<eventlistener name="uzbotUserGroupApplicationAction">
			<eventclassname>wcf\data\user\group\application\UserGroupApplicationAction</eventclassname>
			<eventname>finalizeAction</eventname>
			<listenerclassname>wcf\system\event\listener\UzbotUserGroupApplicationActionListener</listenerclassname>
			<environment>user</environment>
		</eventlistener>
		
		<eventlistener name="uzbotUserGroupApplicationRevokeAction">
			<eventclassname>wcf\action\UserGroupApplicationRevokeAction</eventclassname>
			<eventname>execute</eventname>
			<listenerclassname>wcf\system\event\listener\UzbotUserGroupApplicationActionListener</listenerclassname>
			<environment>user</environment>
		</eventlistener>
	</import>
</data>
